{
    "compilerOptions": {
        "target": "es5",
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "noImplicitThis": true,
        "module": "commonJS",
        "outDir": "build",
        "moduleResolution": "node",
        "declaration": true,
        "jsx": "react",
        "types": ["node", "react", "@emotion/react/types/css-prop"]
    },
    "compileOnSave": true
}