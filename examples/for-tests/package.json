{
    "name": "react-test-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "axios": "^0.21.0",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-router-dom": "^6.2.1",
        "react-scripts": "^5.0.1"
    },
    "scripts": {
        "prep": "cd ../../ && SRC=./examples/for-tests npm run prep-test-app",
        "start": "npm run prep && PORT=3031 react-scripts start",
        "dev": "npx npm-run-all --parallel start server",
        "server": "cd ../../ && npm run server",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
